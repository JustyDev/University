// Простая работа с датой
дата моя_дата;
вывод "Введите дату в формате дд.мм.гггг:";
ввод моя_дата;

// Парсим дату (эмулируется как строка, но предполагаем, что остальной код транспайлера преобразует к объекту Date)
целое день;
целое месяц;
целое год;
день = parseInt(моя_дата.split(".")[0]);
месяц = parseInt(моя_дата.split(".")[1]);
год = parseInt(моя_дата.split(".")[2]);

// Определяем високосный год
если ((год % 4 = 0) && (год % 100 <> 0)) || (год % 400 = 0)
    вывод "Год високосный"
иначе
    вывод "Год обычный"
конецесли

// Получаем дату завтра
целое миллисекунд_сутки;
целое дата_мс;
миллисекунд_сутки = 24 * 60 * 60 * 1000;
дата_мс = (new Date(год, месяц-1, день)).getTime();
дата_мс = дата_мс + миллисекунд_сутки;
целое завтрa_д;
целое завтрa_м;
целое завтрa_г;
завтрa_д = (new Date(дата_мс)).getDate();
завтрa_м = (new Date(дата_мс)).getMonth() + 1;
завтрa_г = (new Date(дата_мс)).getFullYear();

вывод "Дата следующего дня: ", завтрa_д, ".", завтрa_м, ".", завтрa_г
